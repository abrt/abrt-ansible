# Place this file in your /etc/yum.repos.d/ directory

[epel-abrt]
name=Nightly builds of ABRT related projects - abrt, libreport, btparser, faf
baseurl=http://repos.fedorapeople.org/repos/abrt/abrt/epel-$releasever/$basearch/
enabled=1
skip_if_unavailable=1
gpgcheck=0

[epel-abrt-source]
name=Nightly builds of ABRT related projects - abrt, libreport, btparser, faf - Source
baseurl=http://repos.fedorapeople.org/repos/abrt/abrt/epel-$releasever/SRPMS
enabled=0
skip_if_unavailable=1
gpgcheck=0

# faf requires newer versions of following packages than available on EL6

[rmarko-python-psycopg2]
name=Copr repo for python-psycopg2 owned by rmarko
baseurl=https://copr-be.cloud.fedoraproject.org/results/rmarko/python-psycopg2/epel-6-$basearch/
skip_if_unavailable=True
gpgcheck=0
gpgkey=https://copr-be.cloud.fedoraproject.org/results/rmarko/python-psycopg2/epel-6-$basearch/pubkey.gpg
enabled=1

[rmarko-python-jinja2]
name=Copr repo for python-jinja2 owned by rmarko
baseurl=https://copr-be.cloud.fedoraproject.org/results/rmarko/python-jinja2/epel-6-$basearch/
