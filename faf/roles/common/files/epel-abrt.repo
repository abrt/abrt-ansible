gpgcheck=0
gpgkey=https://copr-be.cloud.fedoraproject.org/results/rmarko/python-jinja2/epel-6-$basearch/pubkey.gpg
enabled=1

[rmarko-python-testing.postgresql]
name=Copr repo for python-testing.postgresql owned by rmarko
baseurl=https://copr-be.cloud.fedoraproject.org/results/rmarko/python-testing.postgresql/epel-6-$basearch/
skip_if_unavailable=True
gpgcheck=0
gpgkey=https://copr-be.cloud.fedoraproject.org/results/rmarko/python-testing.postgresql/epel-6-$basearch/pubkey.gpg
enabled=1

[rmarko-python-flask]
name=Copr repo for python-flask owned by rmarko
baseurl=https://copr-be.cloud.fedoraproject.org/results/rmarko/python-flask/epel-6-$basearch/
skip_if_unavailable=True
gpgcheck=0
gpgkey=https://copr-be.cloud.fedoraproject.org/results/rmarko/python-flask/epel-6-$basearch/pubkey.gpg
enabled=1
