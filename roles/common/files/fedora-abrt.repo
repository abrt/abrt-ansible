# Place this file in your /etc/yum.repos.d/ directory

[abrt-devel]
name=Copr repo for devel owned by @abrt
baseurl=https://copr-be.cloud.fedoraproject.org/results/@abrt/devel/fedora-$releasever-$basearch/
type=rpm-md
skip_if_unavailable=True
gpgcheck=1
gpgkey=https://copr-be.cloud.fedoraproject.org/results/@abrt/devel/pubkey.gpg
repo_gpgcheck=0
enabled=1
enabled_metadata=1
